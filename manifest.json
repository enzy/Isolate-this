{
  "name": "Isolate THIS",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Crops a browser window to contain only selected element",
  "homepage_url": "http://www.matejsimek.cz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Isolate an element"
  },
  "permissions": [
    "tabs", "https://*/*", "http://*/*"
  ]
}